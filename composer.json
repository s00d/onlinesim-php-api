{
  "name": "s00d/onlinesim-api",
  "description": "php onlinesim api client",
  "keywords": [
    "onlinesim",
    "api",
    "client"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "s00d",
      "email": "Virus191288@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vlucas/phpdotenv": "^4.1",
    "larapack/dd": "1.*"
  },
  "autoload": {
    "psr-4": {
      "s00d\\OnlineSimApi\\": "src/",
      "Tests\\": "tests/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "files": [
      "./vendor/larapack/dd/src/helper.php"
    ],
    "psr-4": {
      "s00d\\OnlineSimApi\\": "src/",
      "Tests\\": "tests/"
    },
    "classmap": [
      "src/"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
